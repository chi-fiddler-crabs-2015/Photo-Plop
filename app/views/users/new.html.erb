<!-- has form for signup, renders erorrs from shared/errors -->
<h1>Create an account</h1>
<%= form_for :user, url: users_path do |f| %>
  <%= render "shared/errors" %>
  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit "Sign Up" %>
<% end %>
